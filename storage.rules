rules_version = '2';

// NOTE: Firebase Storage is no longer used for file storage
// All files are now stored in Cloudflare R2
// This file is kept for backward compatibility only

service firebase.storage {
  match /b/{bucket}/o {
    // Deny all access since we're using Cloudflare R2
    match /{allPaths=**} {
      allow read, write: if false;
    }
  }
}
